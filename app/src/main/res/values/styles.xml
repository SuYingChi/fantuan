    <!-- 适用于开启滑动返回功能的Activity -->
    <style name="AppTheme.Transparent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <!-- 适用于欢迎界面避免第一次进来白屏或黑屏 -->
    <style name="AppTheme.Transparent.Splash">
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="MatchWrapTv" parent="MatchWrap">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_btn_orange</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:onClick">onClick</item>
        <item name="android:padding">8dp</item>
    </style>

    <style name="AutoWrapTv" parent="AutoWrap">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_btn_orange</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:onClick">onClick</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="translucent" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/translucent_background</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="popup_window_anim">
        <item name="android:windowEnterAnimation">@anim/in</item>
        <item name="android:windowExitAnimation">@anim/out</item>
    </style>



    <style name="loading_dialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/none_color_lib</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="ButtonRoundCorner" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/lib_bg_btn_primay</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="ButtonNOCorner" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/lib_bg_btn_primay</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="ButtonRoundCornerWhite" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/lib_bg_btn_white</item>
        <item name="android:textColor">@color/colorPrimary_lib</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="ButtonRoundCornerWhiteBGTextBlack" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/lib_bg_btn_white</item>
        <item name="android:textColor">@color/text_commen_lib</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="ButtonRoundCornerWhiteBGTextBlackDark" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/lib_bg_btn_white_dark</item>
        <item name="android:textColor">@color/text_commen_lib</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="AppraisalRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable</item>
        <item name="android:gravity">bottom</item>
        <item name="android:minHeight">14dp</item>
        <item name="android:maxHeight">14dp</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:overScrollMode">never</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppraisalRatingBarBig" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable_big</item>
        <item name="android:gravity">bottom</item>
        <item name="android:minHeight">28dp</item>
        <item name="android:maxHeight">28dp</item>
    </style>

    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="BottomAnimDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@color/lucency</item><!--背景透明-->
        <item name="android:background">@color/lucency</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:windowAnimationStyle">@style/ActionSheetDialogAnimation</item><!--动画-->
    </style>

    <style name="ActionSheetDialogAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/bottom_anim_dialog_in</item>
        <item name="android:windowExitAnimation">@anim/bottom_anim_dialog_out</item>
    </style>



</resources>
